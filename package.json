{
  "name": "cra-template-rb",
  "description": "The official React Boilerplate template for Create React App",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-boilerplate/react-boilerplate-cra-template.git"
  },
  "license": "MIT",
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "bugs": {
    "url": "https://github.com/react-boilerplate/react-boilerplate-cra-template/issues"
  },
  "main": "template.json",
  "files": [
    "template",
    "template.json"
  ],
  "keywords": [
    "react",
    "create-react-app",
    "template",
    "typescript",
    "redux",
    "reduxjs",
    "react-redux",
    "react-boilerplate",
    "styled-components"
  ],
  "scripts": {
    "// ---DEFAULT CRA---": "",
    "start": "cross-env react-scripts start env-cmd -f .env",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "// ---TEMPLATE---": "Scripts will be also in the template",
    "start:prod": "npm run build && serve -s build",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/test-generators.ts",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext ts,tsx",
    "lint": "npm run eslint -- src",
    "lint:fix": "npm run eslint -- --fix src",
    "lint:css": "style lint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write src/**/*{js,jsx,ts,tsx,json,css,scss,md}",
    "// ---TESTING TEMPLATE---": "",
    "test:all": "npm run test -- --watchAll=false",
    "test:coverage": "npm run test:all -- --coverage",
    "test:cra": "npm run create:cra-app && cd generated-cra-app && npm run test:generators && npm run lint && npm run checkTs && npm run cleanAndSetup && npm run lint && npm run checkTs",
    "// ---BUILDING TEMPLATE---": "",
    "create:npm-package": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/create-npm-package.script.ts",
    "create:cra-app": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/create-cra-app.script.ts",
    "// ---PUBLISHING---": "",
    "publish:github": "cross-env PUBLIC_URL='/react-boilerplate-cra-template' npm run build && gh-pages -d build",
    "changelog": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/create-changelog.script.ts",
    "release": "standard-version",
    "publish:npm": "npm run create:npm-package && npm publish .cra-template-rb"
  },
  "lint-staged": {
    "*.{ts,tsx,jsx}": [
      "npm run eslint -- src"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run checkTs && lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "20.0.0",
    "@ckeditor/ckeditor5-react": "2.1.0",
    "@fortawesome/fontawesome-svg-core": "1.2.30",
    "@fortawesome/free-solid-svg-icons": "5.14.0",
    "@fortawesome/react-fontawesome": "0.1.11",
    "@hookform/resolvers": "0.0.5",
    "@react-pdf/renderer": "2.0.0-beta.6",
    "@reduxjs/toolkit": "1.3.2",
    "@types/bootstrap": "4.5.0",
    "axios": "0.19.2",
    "bootstrap": "4.5.0",
    "ckeditor": "4.12.1",
    "ckeditor5": "20.0.0",
    "connected-react-router": "6.8.0",
    "env-cmd": "10.1.0",
    "firebase": "7.15.5",
    "firebase-admin": "8.13.0",
    "fontfaceobserver": "2.1.0",
    "formik": "2.1.5",
    "history": "4.10.1",
    "i18next": "19.3.4",
    "i18next-browser-languagedetector": "4.0.2",
    "jquery": "3.5.1",
    "lodash": "4.17.19",
    "moment": "2.27.0",
    "node-sass": "4.14.1",
    "popper.js": "1.16.1",
    "react": "16.13.0",
    "react-app-polyfill": "1.0.6",
    "react-avatar-editor": "11.0.7",
    "react-bootstrap": "0.33.1",
    "react-datepicker": "3.1.3",
    "react-debounce-input": "3.2.2",
    "react-dom": "16.13.0",
    "react-helmet": "6.1.0",
    "react-helmet-async": "1.0.4",
    "react-hook-form": "6.0.8",
    "react-i18next": "11.3.4",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "react-router-redux": "4.0.8",
    "react-toastify": "6.0.8",
    "redux-injectors": "1.3.0",
    "redux-persist": "6.0.0",
